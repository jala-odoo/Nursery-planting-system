<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="nursery_plant_report">
        <t t-foreach="docs" t-as="details">
            <t t-call="web.html_container">
                <t t-call="web.external_layout">
                   <div class="page">
                    <div>
                      <strong>Plant ID:</strong>
                      <span t-field="details.plant_ID"/>
                    </div>
                   </div>
                </t>
            </t>
        </t>
    </template>
</odoo>